{
    "config": {
        "extends": "markdownlint/style/prettier",
        "default": true,
        "relative-links": true,
        "no-duplicate-heading": false,
        "no-inline-html": false,
    },
    "globs": ["**/*.md"],
    "ignores": ["**/node_modules", "**/target"],
    "customRules": ["markdownlint-rule-relative-links"],
}
